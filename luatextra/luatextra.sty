\def\luasetcatcoderange#1#2#3{%
\edef\luaSCR@temp{%
\noexpand\@tempcnta=\the\@tempcnta %
\noexpand\@tempcntb=\the\@tempcntb %
\noexpand\count@=\the\count@ %
\relax %
}%
\@tempcnta=#1\relax %
\@tempcntb=#2\relax %
\count@=#3\relax %
\loop %
\unless\ifnum\@tempcnta>\@tempcntb %
\catcode\@tempcnta=\count@ %
\advance\@tempcnta by 1\relax %
\repeat %
\luaSCR@temp %
}

