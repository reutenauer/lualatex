function inputenc.load(name)
    if name == 'ascii' or name == 'utf8' then
        return
    end
    if not inputenc.data[name] then
        dofile(kpse.find_file("inputenc-"..name..".lua"))
        if inputenc.data[name] then
            inputenc.utf[name] = { }
            for k,v in pairs(inputenc.data[name]) do
                inputenc.utf[name][char(k)] = utfchar(v)
            end
            inputenc.report(format("loading %s input encoding", name))
        else
            inputenc.error("unable to get data from inputenc-"..name..".lua")
        end
    end
end

function inputenc.translate(line, name)
    if name and line then
        local rur = inputenc.utf[name]
        if rur then
            return line:gsub("(.)", rur)
        end
    end
    return line
end

function inputenc.translatetoascii(line)
    if line then
        return line:gsub("(.)", function(s)
            if byte(s) >= 128 then
                inputenc.error(format("warning: non-ascii character number '%s'", byte(s)))
            end
            return utfchar(byte(s))
          end
        )
    end
    return line
end

function inputenc.translatetocurrent(line)
    if inputenc.current == 'ascii' then
        return inputenc.translatetoascii(line)
    else
        return inputenc.translate(line, inputenc.current)
    end
end

function inputenc.enable(name)
    if name == 'ascii' then
        inputenc.report("loading ascii input encoding")
    end
    if name == 'utf8' then
        inputenc.current = 'utf8'
        return
    end    
    if not inputenc.data[name] then
        inputenc.load(name)
    end
    if inputenc.data[name] or name == 'ascii' then
        inputenc.current = name
        callback.register('process_input_buffer', inputenc.translatetocurrent)
    end
end

function inputenc.activate(name)
    local synonym = inputenc.synonyms[name]
    if not synonym then
        inputenc.report(string.format("unknown encoding: %s", name))
        return
    end
    inputenc.enable(synonym)
end

end

